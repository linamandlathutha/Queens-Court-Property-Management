                                 <!--         <li><a asp-area="" asp-controller="ReportApartment" asp-action="Index">Apartment Reports</a></li>-->

                                   
                                </ul>
                            </li>

                        }
                        @if (User.IsInRole("Admin"))
                        {

                            <li class="has-children">
                                <a>Management</a>
                                <ul class="dropdown" style="color:black">
                                    <li>
                                        <a asp-area="Text" asp-controller="Apartment" asp-action="Index" style="color:black">Apartments</a>
                                    </li>


                                    <li>
                                        <a asp-area="Text" asp-controller="Home" asp-action="History" style="color:black">History</a>
                                    </li>



                                    <li>
                                        <a class="Text" asp-area="" asp-controller="Manager" asp-action="Index" style="color:black">Registrations</a>
                                    </li>


                                  
                                    <li>
                                        <a class="Text" asp-area="" asp-controller="Reservations" asp-action="Index" style="color:black"> Reservations</a>
                                    </li>
                                    <li>
                                        <a class="Text" asp-area="" asp-controller="Home" asp-action="History" style="color:black"> Payments</a>
                                    </li>
                                    <li>
                                        <a class="Text" asp-area="" asp-controller="HomeFile" asp-action="ViewFiles" style="color:black"> View Lease-Agreements</a>
                                    </li>
                                    <li><a class="Text" asp-area="" asp-controller="ReportApartment" asp-action="Index" style="color:black">Apartment Reports</a></li>
                                   
                                   
                                </ul>
                            </li>
                           
                           


                        }

                        @if (User.IsInRole("Manager"))
                        {

                              <li class="has-children">
                                <a asp-controller="Apartment" asp-action="Index">Management</a>
                                <ul class="dropdown">
                                    <li><a asp-area="" asp-controller="Apartment" asp-action="Create">Add Apartments</a></li>
                                    <li><a asp-area="" asp-controller="Apartment" asp-action="Index" >List Apartments</a></li>
                                    <li><a asp-area="" asp-controller="Reservations" asp-action="Index">View Reservations</a></li>
                                    <li><a asp-area="" asp-controller="Home" asp-action="ManageOverdueTenants">View Overdue Apartments</a></li>

                                    <li><a asp-area="" asp-controller="Home" asp-action="History">Payments</a></li>
                                    <li><a asp-area="" asp-controller="HomeFile" asp-action="ViewFiles">Lease-Agreements</a></li>
                                    <li><a asp-area="" asp-controller="Tests" asp-action="Index">Logins</a></li>
                                    </ul>
                                    </li>


                          

                        }
                   
                        <!-- Display the Login and Register buttons with the same styling -->
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hello @User.Identity?.Name!</a>
                            </li>
                            <li class="nav-item">
                                <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                                    <button type="submit" class="nav-link btn btn-link text-dark">Logout</button>
                                </form>
                            </li>
                        }
                        else
                        {
                            <li class="nav-item">
                                <a class="nav-link" asp-area="Identity" asp-page="/Account/Register">Register</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="Identity" asp-page="/Account/Login">Login</a>
                            </li>
                        }
                    </ul>

                   



                    <a href="#"
                       class="burger light me-auto float-end mt-1 site-menu-toggle js-menu-toggle d-inline-block d-lg-none"
                       data-toggle="collapse"
                       data-target="#main-navbar">
                        <span></span>
                    </a>
                </div>
            </div>
        </div>
    </nav>


