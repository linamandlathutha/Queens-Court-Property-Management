                        <li class="active"><a asp-area="" asp-controller="Apartment" asp-action="Tesr">Home</a></li>
                      

                         @if (User.Identity.IsAuthenticated && !(User.IsInRole("Admin")) && !(User.IsInRole("Manager")))
